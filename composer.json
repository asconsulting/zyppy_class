{
    "name":"asconsulting/zyppy_class",
    "description":"A module for the Contao CMS to allow for easy selection of pre-configured CSS classes.",
    "homepage": "https://andrewstevens.consulting",
    "keywords":["contao", "class", "css", "selector", "styles", "style sheet", "stylesheet"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Andrew Stevens",
            "homepage":"https://andrewstevens.consulting"
        }
    ],
    "require":{
        "php": ">=7.3",
        "contao/core-bundle": "^4.9",
        "contao-community-alliance/composer-plugin": "~2.4 || ~3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ZyppyClass\\": [
                "system/modules/zyppy_class/library/ZyppyClass"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/zyppy_class": "system/modules/zyppy_class"
            }
        }
    },
    "minimum-stability": "dev"
}
